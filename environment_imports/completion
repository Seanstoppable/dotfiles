if command -v brew > /dev/null; then

  if [ -f `brew --prefix`/etc/bash_completion.d/git-completion.bash ]; then
    source `brew --prefix`/etc/bash_completion.d/git-completion.bash
  fi

  if [ -f `brew --prefix`/etc/bash_completion.d/ag-bashcomp.sh ]; then
    source `brew --prefix`/etc/bash_completion.d/ag.bashcomp.sh 
  fi
  if [ -f $(brew --prefix)/etc/bash_completion ]; then
    source $(brew --prefix)/etc/bash_completion
  fi
  if [ -f `brew --prefix`/etc/bash_completion.d/docker-compose ]; then
    source `brew --prefix`/etc/bash_completion.d/docker-compose 
  fi
fi

if [ -f /usr/local/bin/aws_completer ]; then
  complete -C "/usr/local/bin/aws_completer" aws
fi

